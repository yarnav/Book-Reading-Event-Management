@model IList<Company.Project.Web.Models.EventViewModel>

@{ ViewData["Title"] = "Index"; }
<div class="container">
    @if (Model.Count == 0)
    {
        <div>
            <h4 class="text-info">No Event Created yet</h4>
        </div>
    }
    else
    {
       <partial name="_DisplayEvents" model="@Model"/>
    }

</div>

@* asp-page="/Event/ViewDetails/" asp-route-id="@_event.Id" *@